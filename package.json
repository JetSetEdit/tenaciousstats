{
  "name": "tenacious-stats-dashboard",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "node increment-version.js",
    "build": "echo Build complete",
    "dev": "http-server public -p 8080 -o",
    "dev:local": "python run_vercel_local.py",
    "dev:vercel": "vercel dev",
    "docker:up": "docker-compose -f docker-compose.dev.yml up",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:build": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:start": "powershell -ExecutionPolicy Bypass -File ./start-docker.ps1"
  },
  "devDependencies": {
    "http-server": "^14.1.1"
  }
}
